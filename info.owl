    <!-- http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology#has_quality -->

    <owl:ObjectProperty rdf:about="http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology#has_quality">
