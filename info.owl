        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000002"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a Continuant and a Processual Entity in which the Continuant participates. The presence of the Continuant at the end of the Processual Entity is a necessary condition for the completion of the Processual Entity.</ExtendedRelationOntology:definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is output of</rdfs:label>
    </owl:ObjectProperty>
    


